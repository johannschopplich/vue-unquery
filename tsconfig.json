{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Node",
    "paths": {
      "vue-unquery": ["./src/index.ts"]
    },
    "types": ["vitest"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
