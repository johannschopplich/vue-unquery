    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.10"
  }
